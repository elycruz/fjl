{
  "name": "fjl",
  "version": "1.10.7",
  "description": "Functional Javascript Library",
  "main": "dist/cjs/fjl",
  "module": "dist/es6-module/fjl.mjs",
  "types": "./index.d.ts",
  "scripts": {
    "build": "gulp docs build && npm run test-builds",
    "test-builds": "jest --testMatch **/tests/*-test-*.ts",
    "test": "jest --config ./jest.config.js",
    "readme": "gulp readme",
    "docs": "gulp docs"
  },
  "directories": {
    "dist": "dist",
    "doc": "README.md",
    "test": "tests",
    "lib": "src"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/parser": "^7.4.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-bigint": "^7.4.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.0",
    "@babel/types": "^7.4.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "del": "^4.1.0",
    "eslint": "^5.16.0",
    "gulp": "4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-duration": "0.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "^2.0.7",
    "gulp-if": "^2.0.2",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-uglify": "^3.0.2",
    "jest": "^24.7.1",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^3.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/functional-jslib/fjl.git"
  },
  "keywords": [
    "functional programming",
    "functional",
    "fp",
    "programming",
    "haskell",
    "prelude",
    "combinators"
  ],
  "author": "Ely De La Cruz <elycruz@elycruz.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/functional-jslib/fjl/issues"
  },
  "homepage": "https://github.com/functional-jslib/fjl#readme",
  "dependencies": {},
  "buildConfig": {
    "inputModuleName": "fjl",
    "inputFilePath": "./dist/es/fjl.js",
    "outputFileName": "fjl.js",
    "outputFileNameMjs": "fjl.mjs",
    "outputFileNameMin": "fjl.min.js",
    "srcsGlob": "./dist/es/**/*.js",
    "paths": {
      "dist": "./dist",
      "docs": "./docs"
    },
    "folderNames": {
      "amd": "amd",
      "cjs": "cjs",
      "iife": "iife",
      "umd": "umd",
      "es6Module": "es6-module",
      "package": "package"
    },
    "es6ModuleRollup": {
      "config": {
        "input": "./dist/es/fjl.js",
        "external": []
      },
      "write": {
        "file": "./dist/es6-module/fjl.js",
        "format": "es",
        "sourcemap": true
      }
    },
    "buildAmdOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-amd"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildCjsOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildUmdOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-umd"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildIifeOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ]
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "uglifyOptions": {},
    "readme": {
      "files": [
        "./markdown-fragments/readme-header.md",
        "./markdown-fragments/sections/sections.md",
        "./markdown-fragments/sections/requirements.md",
        "./markdown-fragments/sections/getting-started.md",
        "./markdown-fragments/sections/docs.md",
        "./markdown-fragments-generated/module-and-member-list.md",
        "./markdown-fragments/sections/docs/docs-footer.md",
        "./markdown-fragments/sections/development.md",
        "./markdown-fragments/sections/license.md",
        "./markdown-fragments/sections/resources.md",
        "./markdown-fragments/sections/changelog.md"
      ]
    },
    "jsdoc": {
      "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "templates": {
          "useCollapsibles": false
        }
      },
      "templates": {
        "logo": {
          "url": "http://elycruz.com/fjl-logo.gif",
          "width": "144px",
          "height": "55px",
          "link": "https://github.com/functional-jslib/fjl"
        },
        "footerText": "functional-jslib - BSD 3.0 License - JsDoc Template -> tui-jsdoc-template - by NHN Entertainment - Frontend Development Lab"
      }
    }
  }
}
