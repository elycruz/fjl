{
  "compilerOptions": {
    "module": "ESNext",
    "lib": [
      "esnext"
    ],
    "noImplicitAny": false,
    "target": "ES2018",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
