{
  "compilerOptions": {
    "module": "ESNext",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "noImplicitAny": false,
    "target": "ES2018",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "exclude": [
    "**/node_modules",
    "**/dist"
  ]
}
