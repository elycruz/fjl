{
  "name": "fjl-input-filter",
  "version": "1.2.9",
  "license": "BSD-3-Clause",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "build": "node ../../node-scripts/tasks/build-package.js fjl",
    "build:all": "npm run build:es && npm run build:cjs && npm run build:umd && npm run build:iife",
    "test": "jest --config ./jest.config.js",
    "docs": "echo \"Not yet implemented.\"",
    "build:es": "tsc --project tsconfig.prod.es.json",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:umd": "tsc --project tsconfig.prod.umd.json",
    "build:iife": "rollup -c ./rollup.config.js"
  },
  "dependencies": {
    "fjl": "^1.12.6",
    "fjl-validator": "^0.7.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/plugin-transform-modules-amd": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/jest": "^24.9.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-upgrade": "1.0.1",
    "del": "^5.0.0",
    "gulp": "4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-duration": "0.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.7",
    "gulp-if": "^3.0.0",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-uglify": "^3.0.2",
    "jest": "^24.8.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "tui-jsdoc-template": "^1.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./node_modules/regenerator-runtime/runtime"
    ]
  },
  "buildConfig": {
    "inputModuleName": "fjlInputFilter",
    "inputFilePath": "./src/fjlInputFilter.js",
    "outputFileName": "fjl-input-filter.js",
    "outputFileNameMjs": "fjl-input-filter.mjs",
    "outputFileNameMin": "fjl-input-filter.min.js",
    "srcsGlob": "./src/**/*.js",
    "paths": {
      "dist": "./dist",
      "docs": "./docs"
    },
    "folderNames": {
      "amd": "amd",
      "cjs": "cjs",
      "iife": "iife",
      "umd": "umd",
      "es6Module": "es6-module",
      "package": "package"
    },
    "es6ModuleRollup": {
      "config": {
        "input": "./src/fjlInputFilter.js",
        "external": [
          "fjl",
          "fjl-mutable",
          "fjl-validator"
        ]
      },
      "write": {
        "file": "./dist/es6-module/fjl-input-filter.js",
        "format": "es",
        "sourcemap": true
      }
    },
    "buildAmdOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-amd"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildCjsOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildUmdOptions": {
      "babel": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-umd"
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "buildIifeOptions": {
      "rollup": {
        "external": [
          "fjl",
          "fjl-mutable",
          "fjl-validator"
        ]
      },
      "babel": {
        "babelrc": false,
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ]
        ],
        "exclude": [
          "node_modules/**/*"
        ]
      }
    },
    "uglifyOptions": {},
    "readme": {
      "files": [
        "./markdown-fragments/readme-header.md",
        "./markdown-fragments/sections/sections.md",
        "./markdown-fragments/sections/requirements.md",
        "./markdown-fragments/sections/getting-started.md",
        "./markdown-fragments/sections/docs.md",
        "./markdown-fragments-generated/module-and-member-list.md",
        "./markdown-fragments/sections/docs/docs-footer.md",
        "./markdown-fragments/sections/development.md",
        "./markdown-fragments/sections/license.md",
        "./markdown-fragments/sections/resources.md",
        "./markdown-fragments/sections/changelog.md"
      ]
    },
    "jsdoc": {
      "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "templates": {
          "useCollapsibles": false
        }
      },
      "templates": {
        "logo": {
          "url": "http://elycruz.com/fjl-logo-v2.svg",
          "width": "144px",
          "height": "55px",
          "link": "https://github.com/functional-jslib/fjl-input-filter"
        },
        "footerText": "fjl-input-filter library - BSD 3.0 License - JsDoc Template -> tui-jsdoc-template - by NHN Entertainment - Frontend Development Lab"
      }
    }
  }
}
