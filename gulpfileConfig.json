{
  "paths": {
    "buildPathRoot": "./dist",
    "cjsBuildPath": "cjs",
    "amdBuildPath": "amd",
    "iifeBuildPath": "iife",
    "umdBuildPath": "umd"
  },
  "buildAmdOptions": {
    "babel": {
      "plugins": [
        "transform-es2015-modules-amd"
      ]
    }
  },
  "buildCjsOptions": {
    "babel": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "buildUmdOptions": {
    "babel": {
      "plugins": [
        "transform-es2015-modules-umd"
      ]
    }
  },
  "tests": {
    "srcs": [
      "./tests/**/test-*.js"
    ],
    "babel": {
      "presets": ["latest"],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "mocha": {
      "compilers": [
        "js:babel-register"
      ],
      "require": [
        "babel-polyfill"
      ]
    }
  },
  "uglifyOptions": {
    "passed": 2,
    "mangle": {
      "except": [
        "Applicable",
        "Bifunctor",
        "Comonad",
        "Extractable",
        "Profunctor",
        "Applicative",
        "Chainable",
        "Extendable",
        "Functor",
        "Either",
        "Identity",
        "Maybe",
        "Just",
        "Nothing",
        "Monad"
      ]
    }
  }
}