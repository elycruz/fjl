{
  "paths": {
    "buildPathRoot": "./dist",
    "cjsBuildPath": "cjs",
    "amdBuildPath": "amd",
    "iifeBuildPath": "iife",
    "umdBuildPath": "umd",
    "markdownFragsPath": "./markdown-fragments/"
  },
  "buildAmdOptions": {
    "babel": {
      "babelrc": false,
      "plugins": [
        "transform-es2015-modules-amd"
      ]
    }
  },
  "buildCjsOptions": {
    "babel": {
      "babelrc": false,
      "presets": [
        "es2015"
      ],
      "plugins": [
        "babel-plugin-add-module-exports"
      ]
    }
  },
  "buildUmdOptions": {
    "babel": {
      "babelrc": false,
      "presets": [
        "env"
      ],
      "plugins": [
        "babel-plugin-add-module-exports",
        "transform-es2015-modules-umd"
      ]
    }
  },
  "buildIifeOptions": {
    "babel": {
      "babelrc": false,
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  },
  "tests": {
    "srcs": [
      "./tests/for-server/**/test-*.js"
    ],
    "babel": {
      "presets": [
        "latest"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "mocha": {
      "compilers": [
        "js:babel-register"
      ],
      "require": [
        "babel-polyfill"
      ]
    }
  },
  "uglifyOptions": {},
  "readme": [
    "./markdown-fragments/readme-header.md",
    "./markdown-fragments/sections/sections.md",
    "./markdown-fragments/sections/requirements.md",
    "./markdown-fragments/sections/getting-started.md",
    "./markdown-fragments/sections/docs.md",
    "./markdown-fragments-generated/module-and-member-list.md",
    "./markdown-fragments/sections/docs/docs-footer.md",
    "./markdown-fragments/sections/motivations.md",
    "./markdown-fragments/sections/development.md",
    "./markdown-fragments/sections/license.md",
    "./markdown-fragments/sections/resources.md",
    "./markdown-fragments/sections/changelog.md"
  ],
  "jsdoc": {
    "opts": {
      "template": "node_modules/tui-jsdoc-template",
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "templates": {
        "useCollapsibles": false
      }
    },
    "templates": {
      "logo": {
        "url": "http://elycruz.com/fjl-logo-v2.svg",
        "width": "144px",
        "height": "55px",
        "link": "https://github.com/functional-jslib/fjl"
      },
      "footerText": "fjl library - BSD 3.0 License - JsDoc Template -> tui-jsdoc-template - by NHN Entertainment - Frontend Development Lab"
    }
  }
}
